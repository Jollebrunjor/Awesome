                                    <td colspan="3" style=""> @bet.UserBet.Finalist1</td>
                                    <td colspan="3">@bet.UserBet.Finalist2 </td>
                                    <td colspan="3"></td>
                                </tr>
                            </tbody>
                            }
                        </table>
                    </div>
                </td>
                <tr data-toggle="collapse" data-target="#goals" class="accordion-toggle showResultsRow">
                    <td colspan="9">
                        <div>
                            <table style="margin: auto">
                                <tr>
                                    <td>Totalt antal mål i turneringen (utslagsfrågan)</td>
                                    <td width="50px"></td>
                                    <td class="">@Model.CurrentUserBet.TotalGoals</td>
                                </tr>
                            </table>
                        </div>
                    </td>
                </tr>
                <td colspan="12" class="hiddenRow">
                    <div class="accordian-body collapse" id="goals">
                        <table class="table table-striped">
                            <thead>
                                @for (var i = 0; i < Model.OtherUsersBet.Count; i++)
                                {
                                    if ((i % 4) == 0)
                                    {
                                        @:<tr class="text-center">
                                    }
                                    <td class="otherBets">
                                        <div>
                                            <b>@Model.OtherUsersBet[i].LoginName: </b>@Model.OtherUsersBet[i].UserBet.TotalGoals
                                        </div>
                                    </td>

                                    if ((i % 4) == 3)
                                    {
                                       @:</tr>
                                    }
                                }
                            </thead>
                        </table>
                    </div>
                </td>
            </table>
        </div>
    </div>
</div>

<script>

    $("showUserQuarterFinalBet").on("click", "span.btn", function () {
        var childId = $(this).parents("tr").prop("id");
        $(".child-" + childId).toggle("slow");
    });


</script>
